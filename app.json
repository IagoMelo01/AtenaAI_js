{
  "expo": {
    "name": "AtenaAI",
    "slug": "AtenaAI",
    "scheme": "atenaai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "br.edu.atenas.AtenaAI",
      "supportsTablet": true,
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Atena uses your camera so you can capture and send photos or documents inside the AI chat.",
        "NSMicrophoneUsageDescription": "Atena uses your microphone to record and send voice messages during the AI chat."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["RECORD_AUDIO"],
      "edgeToEdgeEnabled": true,
      "package": "br.edu.atenas.AtenaAI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3bf87d1b-72c4-4f33-a497-efd510ddd287"
      }
    }
  }
}
